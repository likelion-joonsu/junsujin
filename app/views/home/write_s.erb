<div class="container">
    <% @writing.each. do |writing| %>
    <%= @writing.subject%><br>
    <%=@writing.stuff%><br>
    <%=@writing.created_at%><br>
    <%end%>
</div>


<div class = "container">
    <form action = "/write_s" method="WRITING" enctype="multipart/form-data">
      <div class="form-group">
        <label for="exampleInputEmail1">제목</label>
        <input type="text" name="subject" class="form-control" id="exampleInputEmail1" placeholder="제목">
      </div>
      <label for="exampleInputEmail1">내용</label>
      <textarea name = "stuff" class="form-control" rows="5" id="stuff"></textarea>    

      <div class="form-group"  style="margin-top:20px;">
        <label for="exampleInputFile">이미지 첨부</label>
        <input type="file" name = "img_file_s" id="exampleInputFile">
      </div>
      
      <button type="submit" class="btn btn-default" style="margin-top:20px;">등록하기</button>
    </form>

</div>